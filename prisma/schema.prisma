datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}
generator client {
    provider = "prisma-client-js"
}

model User {
    id      String   @id @default(auto()) @map("_id") @db.ObjectId
    name    String
    email   String
    password   String
}

model UserAuth {
    id      String   @id @default(auto()) @map("_id") @db.ObjectId
    name    String
    email   String
    image String
}